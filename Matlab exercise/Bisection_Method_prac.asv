clc;
clear all;
f = @(x) x^3 - x-5;
a = 0;
b = 2;
tol = 0.0001;
if (f(a)*f(b)<0)
    for i = 1 :10000000000000
    c = (a+b)/2;
    if (f(a) * f(c) < 0)
        b = c;
    else 
        a = c;
    end
    if (abs(a-b)<tol)
        break;
    end
    end
fprintf("The Root is %f",a);
else
    fprintf("There is No Root");
end
